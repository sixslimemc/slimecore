dispatch minecraft:storage[slimecore:hook] to struct Hook {
    start: (super::hook::info::rebuild::start::Hook | super::hook::info::load::start::Hook),
    end: (super::hook::info::rebuild::end::Hook | super::hook::info::load::end::Hook),
    load: super::hook::info::call::load::Hook,
    entrypoint: super::hook::info::call::entrypoint::Hook,
    preload_entrypoint: super::hook::info::call::preload_entrypoint::Hook,
    uninstall: super::hook::info::call::uninstall::Hook,
    disable: super::hook::info::call::disable::Hook,
    entrypoints: super::hook::info::load::entrypoints::Hook,
    loads: super::hook::info::load::loads::Hook,
    preload_entrypoints: super::hook::info::load::preload_entrypoints::Hook,
}